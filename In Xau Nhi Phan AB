#include <bits/stdc++.h>
using namespace std;

void xauNhiPhanAB(int n){
    
    string s(n,'A');
    
    while(true){
    int i=n-1;
    cout<<s<<endl;
        
    while(i>=0 && s[i]=='B'){
        s[i]='A';
        --i;
    }
    if(i<0) break;
    else{
        s[i]='B';
    }
}
    
}

int main(){
    int n;
    cin >> n ;
    xauNhiPhanAB(n);
    return 0;
}
