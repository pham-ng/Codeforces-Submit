#include<bits/stdc++.h>
using namespace std;


int main(){
    int t;
    cin>>t;
    
    while(t--){
        int n;
        cin>>n;
        vector<int>a(n);
        for(int i=0;i<n;i++){
            a[i]=n-i;
        }
        do{
            for(int i=0;i<n;i++){
                cout<<a[i];
            }
            cout<<endl;
            
        }while(prev_permutation(a.begin(),a.end()));
        
    }
    return 0;
}
