#include<bits/stdc++.h>
using namespace std;

int UCLN(int a,int b){
    if(b==0) return a;
    return UCLN(b,a%b);
}

int main(){
    int a,b;
    cin>>a>>b;
    int result=UCLN(a,b);
    cout<<"Uoc chung lon nhat: "<<result;
    
    return 0;
}
