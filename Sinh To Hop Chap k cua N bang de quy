#include<bits/stdc++.h>
using namespace std;

void toHopChap(vector<int>&a,int n,int k,int i){
    int start = (i == 0) ? 1 : a[i-1] + 1;
    for(int j=start;j<=n-k+i+1;j++){
        a[i]=j;
        if(i==k-1) {
            for(int q=0;q<k;q++){
                cout<<a[q];
            }
            cout<<endl;
        }
        else{
            chinhHopChap(a,n,k,i+1);
        }
    }
}

int main(){
    int n,k;
    cin>>n>>k;
    vector<int>a(k);
    toHopChap(a,n,k,0);
    return 0;
}
